<!DOCTYPE html>
<%@ page import="org.weather.server.*" %>
<html lang="en">
<head>

  <!-- Basic Page Needs
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <meta charset="utf-8">
  <title>Baking Weather</title>
  <meta name="description" content="Baking Weather">
  <meta name="author" content="Tony Kavanagh">

  <!-- Mobile Specific Metas
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- FONT
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <link href="//fonts.googleapis.com/css?family=Raleway:400,300,600" rel="stylesheet" type="text/css">

  <!-- CSS
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <link rel="stylesheet" href="css/normalize.css">
  <link rel="stylesheet" href="css/skeleton.css">

  <!-- Favicon
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <link rel="icon" type="image/png" href="images/favicon.png">

</head>
<body>

<%
    WeatherBaking baking = new WeatherBaking();
    baking.processUrl();
%>


  <!-- Primary Page Layout
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <div class="container">
    <div class="row">
      <center>
      <p><b>Baking in <%= baking.getCity() %></b></p>
      </center>
    </div>
    <!-- START DATA ROWS -->
    <div class="row">
      <div class="four columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Temperature:</p>
      </div>
      <div class="two columns">
        <p><%= baking.getTemperature() %></p>
      </div>
      <div class="four columns">
        <p>foo</p>
      </div>
    </div>
    <div class="row">
      <div class="four columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Humidity:</p>
      </div>
      <div class="two columns">
        <p><%= baking.getHumidity() %></p>
      </div>
      <div class="four columns">
        <p>foo</p>
      </div>
    </div>
    <div class="row">
      <div class="four columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Wind Speed:</p>
      </div>
      <div class="two columns">
        <p><%= baking.getWind() %></p>
      </div>
      <div class="four columns">
        <p>foo</p>
      </div>
    </div>
    <div class="row">
      <div class="four columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Wind Direction:</p>
      </div>
      <div class="two columns">
        <p><%= baking.getWindDir() %></p>
      </div>
      <div class="four columns">
        <p>foo</p>
      </div>
    </div>
    <div class="row">
      <div class="four columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Barometer:</p>
      </div>
      <div class="two columns">
        <p><%= baking.getPressure() %></p>
      </div>
      <div class="four columns">
        <p>foo</p>
      </div>
    </div>
    <div class="row">
      <div class="four columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Conditions:</p>
      </div>
      <div class="two columns">
        <p><%= baking.getSky() %></p>
      </div>
      <div class="four columns">
        <p>foo</p>
      </div>
    </div>
    <div class="row">
      <div class="four columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Current Time:</p>
      </div>
      <div class="two columns">
        <p><%= baking.getTime() %></p>
      </div>
      <div class="four columns">
        <p>foo</p>
      </div>
    </div>
    <div class="row">
      <div class="four columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Current Date:</p>
      </div>
      <div class="two columns">
        <p><%= baking.getDate() %></p>
      </div>
      <div class="four columns">
        <p>foo</p>
      </div>
    </div>
    <!-- END DATA ROWS -->
    <!-- START BAKING COMMENTS -->
    <p> <br> </p>

    <div class="row">
      <div class="two columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Dough:</p>
      </div>
      <div class="five columns">
        <p>May need extra flour</p>
      </div>
      <div class="three columns">
        <p>foo</p>
      </div>
    </div>
    <div class="row">
      <div class="two columns">
        <p>foo</p>
      </div>
      <div class="two columns">
        <p>Baking:</p>
      </div>
      <div class="five columns">
        <p>Tough day to bake</p>
      </div>
      <div class="three columns">
        <p>foo</p>
      </div>
    </div>
    <!-- END BAKING COMMENTS -->
    <p> <br> </p>
    <div class="row">
      <center>
        <table style="width:60%" border="0">
          <tr>
            <td><img src="images/spacer.gif" height="1" width="3"></td> 
            <td><img src="images/feather-60x60.png" alt="Feather" height="60" width="60"></td> 
            <td></td> 
        </table>
      </center>
    </div>
  </div>

<!-- End Document
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
</body>
</html>
